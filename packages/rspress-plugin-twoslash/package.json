{
  "name": "@ap0nia/rspress-plugin-twoslash",
  "private": true,
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.js",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./theme": {
      "types": "./src/theme/index.d.ts",
      "require": "./src/theme/index.jsx",
      "import": "./src/theme/index.jsx",
      "default": "./src/theme/index.jsx"
    },
    "./floating-components": {
      "require": "./src/floating-components/index.js",
      "import": "./src/floating-components/index.js",
      "default": "./src/floating-components/index.js"
    },
    "./*.css": {
      "require": "./*.css",
      "import": "./*.css",
      "default": "./*.css"
    },
    "./*": {
      "types": "./src/*.d.ts",
      "require": "./src/*.js",
      "import": "./src/*.js",
      "default": "./src/*.js"
    }
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.28",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@legendapp/state": "3.0.0-beta.19",
    "@rspress/runtime": "^1.37.2",
    "@rspress/theme-default": "^1.37.2",
    "@shikijs/rehype": "^1.23.1",
    "@shikijs/twoslash": "^1.23.1",
    "@volar/language-core": "^2.4.10",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-mdxjs-esm": "^2.0.1",
    "mdast-util-to-hast": "^13.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-stringify": "^10.0.1",
    "svelte2tsx": "^0.7.26",
    "tailwind-merge": "^2.5.4",
    "twoslash": "^0.2.12",
    "twoslash-protocol": "^0.2.12",
    "typescript": "^5.6.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@rspress/shared": "^1.37.2",
    "@types/bun": "^1.1.6",
    "@types/hast": "^3.0.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "shiki": "^1.9.0",
    "twoslash-vue": "^0.2.12",
    "unified": "^10.1.2"
  }
}
